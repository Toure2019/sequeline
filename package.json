{
  "name": "sar_sw_api-back_js",
  "version": "2.0.0-15-SNAPSHOT-2",
  "description": "ASR Back API Project",
  "scripts": {
    "start": "npm run serve",
    "prebuild": "node_modules/.bin/rimraf dist",
    "start:dev": "./node_modules/.bin/ts-node-dev src/server.ts",
    "build": "npm run build-ts && npm run lint",
    "buildProd": "./node_modules/.bin/tsc -p .",
    "testAndStart": "npm run build && npm run test && npm run start",
    "seedAndWatch": "npm run watch-node",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "npm run test:unit",
    "test:unit": "node_modules/.bin/jest test/unit --coverage --verbose",
    "test:e2e": "NODE_ENV=development ./node_modules/.bin/jest test/e2e --coverage --verbose",
    "seed:undo": "./node_modules/.bin/sequelize db:seed:undo:all",
    "seed": "./node_modules/.bin/sequelize db:seed:all",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "migrate:rollback": "./node_modules/.bin/sequelize db:migrate:undo",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "./node_modules/.bin/tsc",
    "watch-ts": "./node_modules/.bin/tsc -w",
    "lint": "./node_modules/.bin/tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/md5": "^2.1.33",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.13",
    "@wastimy/joi-middleware": "^2.2.0",
    "async": "^3.2.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "fast-csv": "^4.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "pg": "^7.18.2",
    "php-serialize": "4.0.2",
    "rimraf": "^3.0.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^3.0.8",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/compression": "^1.7.0",
    "@types/concurrently": "^4.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/eslint": "^6.1.8",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.11",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.32",
    "@types/node-sass": "^4.11.0",
    "@types/passport": "^1.0.3",
    "@types/pug": "^2.0.4",
    "@types/request": "^2.48.4",
    "@types/request-promise": "^4.1.46",
    "@types/shelljs": "^0.8.7",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chai": "^4.2.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "shelljs": "^0.8.3",
    "sqlite3": "^4.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^3.8.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4,
    "useRelativePath": true
  }
}
